# URL for the resume link (used in Vite frontend)
VITE_RESUME_URL=your_resume_url_here

# Google API key (used for Gemini and potentially other Google services)
# Note: This might be the same key used in the backend
GOOGLE_API_KEY=your_google_api_key_here

# Specific Gemini model to use (optional, defaults might be used if not set)
GEMINI_MODEL=gemini-pro # Or your desired model

# Pinecone API key (used for vector database)
# Note: This might be the same key used in the backend
PINECONE_API_KEY=your_pinecone_api_key_here

# Google Programmable Search Engine ID (CX)
CX=your_google_programmable_search_engine_id_here

# Google Programmable Search Engine API Key
GPSE_API_KEY=your_google_programmable_search_engine_api_key_here

# URL for the backend chatbot API (used in Vite frontend)
VITE_BOT_URL=http://localhost:8000/api/chat # Or your deployed backend URL 